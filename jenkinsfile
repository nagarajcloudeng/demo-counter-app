pipeline{
    
    agent any 
    
    stages {
        
        stage('Git Checkout'){
            
          steps{
                
             script{
                    
              git branch: 'main', url: 'https://github.com/nagarajcloudeng/demo-counter-app.git'

                }
            }
        }
          
         stage ('unit test'){
           
           steps{
         
             sripts{
                 
              sh 'mvn test'
          }
   
        }
     }
   
    }

 }  
